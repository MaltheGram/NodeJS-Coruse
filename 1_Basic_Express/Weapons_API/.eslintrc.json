{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-case-declarations": "off",
        "no-console": "off",
        "no-useless-escape": "off",
        "camelcase": "warn"
    }
}
